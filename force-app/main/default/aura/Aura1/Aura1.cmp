<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="global" >
      <aura:attribute name="empname" type="string" default="swapna" />
      <aura:attribute name="numberofemployees" type="integer" default="200" required="true" description="" />
        <aura:attribute name="Joiningdate" type="DateTime" default="2020-10-11" />
      <aura:attribute name="Joiningdatetime" type="DateTime" default="2020-10-11" />
      <aura:attribute name="salary" type="Decimal" default="43555" />
      <aura:attribute name="Isreceived" type="Boolean" default="true" />

     
    
          <div class= "slds-text-align_left">
    
              <span><b>Employ name:</b>Employ name: {!v.empname}</span><br/>
               <span><b>Number of employees:</b> {!v.numberofemployees}</span><br/>
              <span><b>Joining date:</b> {!v.Joiningdate}</span><br/>
               <span><b>Joining date time:</b> {!v.Joiningdatetime}</span><br/>
               <span><b>Salary:</b> {!v.salary}</span><br/>
               <span><b>Is salary received:</b> {!v.Isreceived}</span>
    
    
    </div>
</aura:component>